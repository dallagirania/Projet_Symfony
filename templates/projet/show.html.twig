{% block title %}Projet{% endblock %}

{% block body %}
    <h1>Projet</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ projet.titre }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ projet.type }}</td>
            </tr>
            <tr>
                <th>Echeance</th>
                <td>{{ projet.echeance }}</td>
